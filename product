product = []
name = []
price = []
#total =[]

while True:
    good = input('please enter your product: ')
    if good == 'q':
        break
    money = input('what is the price: ')

    product.append([good, money])

#    total.append(good) 可寫成上面那種
#    total.append(money)


print(product)

for p in product:
    print('the good is: ', p[0], 'price is: ', p[1] )                       
